<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="641.0" prefWidth="777.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Thief.Controller">
    <children>
        <Label layoutX="14.0" layoutY="31.0" text="Objects : ">
            <font>
                <Font name="Serif Bold" size="17.0" />
            </font>
        </Label>
        <Button layoutX="15.0" layoutY="65.0" mnemonicParsing="false" onAction="#sample" text="Sample">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <TextArea fx:id="objectsTA" layoutX="107.0" layoutY="24.0" prefHeight="301.0" prefWidth="597.0" promptText="in each line write the id of the object and it's value / weight eg :&#10;&#13;gold : 50/100 &#10;&#13;TV: 20/3000 &#10;&#13;PC: 30 / 1000 &#10;&#13;couch : 30 / 5000&#10;&#13;toothPaste : 2 / 10">
            <font>
                <Font name="System Bold" size="19.0" />
            </font>
        </TextArea>
        <Button layoutX="354.0" layoutY="337.0" mnemonicParsing="false" onAction="#solve" text="solve">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <Label fx:id="result" layoutX="15.0" layoutY="414.0">
            <font>
                <Font name="Serif Bold" size="17.0" />
            </font>
        </Label>
      <Label layoutX="15.0" layoutY="131.0" text="the max : ">
         <font>
            <Font name="Serif Bold" size="17.0" />
         </font>
      </Label>
      <TextField fx:id="maxTF" layoutX="8.0" layoutY="162.0" prefHeight="25.0" prefWidth="87.0" />
      <Button layoutX="706.0" layoutY="26.0" mnemonicParsing="false" onAction="#back" text="&lt;-" />
    </children>
</AnchorPane>
